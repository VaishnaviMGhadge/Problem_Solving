def FreqCounter(arr):
    l=[]
    for i in range(len(arr)):
        c=1
        for j in range(i+1,len(arr)):
            if(arr[i]==arr[j]):
                c+=1
            else:
                pass
        #if(arr[i] not in l):
        l.append(arr[i])
        l.append(c)
        c=1
    return l

        


if __name__=="__main__":
    arr=[1,2,1,3,2]
    ans=FreqCounter(arr)
    print(ans)





