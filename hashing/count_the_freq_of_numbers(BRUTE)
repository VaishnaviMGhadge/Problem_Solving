def freq_counter(arr):
    max_ele=max(arr)+1
    hash_arr=[0]*max_ele
    for i in range(len(arr)):
        if(hash_arr[arr[i]]):
            hash_arr[arr[i]]+=1
        else:
            hash_arr[arr[i]]=1
    return hash_arr


if __name__=="__main__":
    ans=freq_counter([1,2,1,4,5,6,6,6,6,6,6,6,6,6,6,6,8,9,10,1])
    print(ans)






